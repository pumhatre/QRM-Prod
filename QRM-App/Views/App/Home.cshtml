<style>
    .testClass {
        z-index: -10;
    }

    .wrap {
        white-space: normal;
    }


    .ui-grid-row:last-child > div > div {
        border-bottom: 0.5px solid #717171 !important;
    }

    /*.ui-grid-row > div > div > div:last-child {
        border-right: 1px solid black;
    }*/
    /*this datalist  hide thead text box command unwanted message*/
    .ui-grid-pager-row-count-label {
        display: none;
    }

    .ui-grid-pager-control-input {
        display: none !important;
    }

    .ui-grid-pager-max-pages-number {
        display: none;
    }

    .ui-grid-cell {
        height: 40px !important;
    }

    @@media screen and (min-width: 1080px) {
        .modal-dialog {
            width: 1080px; /* New width for default modal */
        }

        .modal-sm {
            width: 700px; /* New width for small modal */
        }
    }

    @@media screen and (min-width: 1080px) {
        .modal-lg {
            width: 1080px; /* New width for large modal */
        }
    }

    #healthReportModal .modal-body {
        height: 85vh !important;
    }
    /*End */
    .grid .msg {
        opacity: 1;
        position: absolute;
        top: 20%;
        left: 20%;
        width: 60%;
        height: 50%;
        background-color: #eee;
        border-radius: 4px;
        border: 1px solid #555;
        text-align: center;
        font-size: 24px;
        display: table;
    }

        .grid .msg span {
            display: table-cell;
            vertical-align: middle;
        }

    /*this datalist  hide thead text box command unwanted message*/
    .ui-grid-pager-row-count-label {
        display: none;
    }

    .ui-grid-pager-control-input {
        display: none !important;
    }

    .ui-grid-pager-max-pages-number {
        display: none;
    }
    /*End */

    #mGrid {
        width: 100% !important;
        height: 320px !important;
    }

    .tileReview {
        width: 100%;
        display: inline-block;
        box-sizing: border-box;
        /*background: #fff;*/
        padding: 20px;
        margin-bottom: 10px;
    }

        .tileReview .tileReview {
            margin-top: 0px;
        }

    .tileReview {
        color: #fff;
    }

        .tileReview:hover {
            color: #000000;
        }
</style>

<div class="x_panel" role="main" ng-app="home">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Home</h3>
            </div>
        </div>
        <div class="row" ng-controller="homeCtrl">
            <div class="col-md-12 col-sm-6 col-xs-12">
                <div class="x_panel">
                    <div class="x_content fix">
                        <div class="col-md-12" role="tabpanel" data-example-id="togglable-tabs">
                            <ul id="myTab" class="nav nav-tabs bar_tabs" role="tablist">
                                <li role="presentation" class="active">
                                    <a href="#tab_content1" target="_self" id="home-tab" role="tab" data-toggle="tab" aria-expanded="true">Project Performance Charts</a>
                                </li>
                                <li role="presentation" class="">
                                    <a href="#tab_content2" ng-click="LoadMyProject()" role="tab" target="_self" id="Project-tab" data-toggle="tab" aria-expanded="false">My Projects</a>
                                </li>
                                <li role="presentation" class="">
                                    <a href="#tab_content3" role="tab" ng-click="LoadMyUpcomingReview()" target="_self" id="Project-tab" data-toggle="tab" aria-expanded="false">Upcoming Metrics Submission</a>
                                </li>
                                <li role="presentation" class="">
                                    <a href="#tab_content4" ng-click="getSavedReports()" role="tab" target="_self" id="Project-tab" data-toggle="tab" aria-expanded="false">My Saved Reports</a>
                                </li>
                                <li role="presentation" class="">
                                    <a href="#tab_content5" ng-click="LoadMyProjectUploadData()" role="tab" target="_self" id="Project-tab" data-toggle="tab" aria-expanded="false">My Uploads</a>
                                </li>

                            </ul>
                            <div id="myTabcontent" class="tab-content">
                                <div role="tabpanel" class="tab-pane fade active in" id="tab_content1" aria-labelledby="home-tab">
                                    <div class="col-md-12 col-sm-4 col-xs-12">
                                        <div class="x_panel tile fixed_height_420">
                                            <div class="x_content">
                                                <div class="form-group">
                                                    <div class="row" ng-hide="displayLSL">
                                                        <div class="col-md-6">
                                                            <div class="text-center">
                                                                <br />
                                                                <h4 style="text-align:center;">Effort Variance(%)</h4>
                                                                <canvas id="EffortVariancePercent" class="chart chart-bar"
                                                                        chart-data="EffortVariancePercentData"
                                                                        chart-labels="EffortVariancePercentLabels"
                                                                        chart-series="EffortVariancePercentSeries"
                                                                        chart-options="EffortVariancePercentOptions"
                                                                        chart-colors="EffortVariancePercentColors"
                                                                        chart-dataset-override="EffortVariancePercentOverride"></canvas>


                                                            </div>
                                                            <div>
                                                                <p>&nbsp;&nbsp;&nbsp;&nbsp;LSL: -10% &nbsp;&nbsp;&nbsp; USL: 10%</p>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="text-center">
                                                                <br />
                                                                <h4 style="text-align:center;">Rework(%)</h4>
                                                                <canvas id="ReworkPercent" class="chart chart-bar"
                                                                        chart-data="ReworkPercentData"
                                                                        chart-labels="ReworkPercentLabels"
                                                                        chart-series="ReworkPercentSeries"
                                                                        chart-options="ReworkPercentOptions"
                                                                        chart-colors="ReworkPercentColors"
                                                                        chart-dataset-override="ReworkPercentOverride"></canvas>


                                                            </div>
                                                            <div>
                                                                <p>&nbsp;&nbsp;&nbsp;&nbsp;LSL: 0% &nbsp;&nbsp;&nbsp; USL: 10%</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row" ng-show="ShowNoData">
                                                        <p style="text-align:center;font-size:20px;"> No Data</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane fade" id="tab_content2" aria-labelledby="Project-tab">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="x_panel">
                                                <div class="x_content">
                                                    <div class="col-md-12">
                                                        <div id="mprojectGrid" ui-grid="projectGrid" class="gridDiv grid" ui-grid-cellNav ui-grid-resize-columns ui-grid-move-columns ui-grid-pinning ui-grid-selection ui-grid-exporter ui-grid-auto-resize style="height: 200px !important; width:100%">
                                                            <div class="grid-msg-overlay" ng-show="loading">
                                                                <div class="msg">
                                                                    <span>
                                                                        Loading Data...
                                                                        <i class="fa fa-spinner fa-spin"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                            <div class="grid-msg-overlay" ng-hide="loading || projectGrid.data.length">
                                                                <div class="msg">
                                                                    <span>No Data</span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane fade" id="tab_content3" aria-labelledby="Project-tab">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="x_panel">
                                                <div class="x_content">
                                                    <div class="col-md-12">
                                                        <h1>
                                                            My Upcoming Metrics Submission
                                                        </h1>
                                                        <div class="container" ng-repeat="x in projectReviewData">
                                                            <div class="row" ng-if="$even">
                                                                <div class="col-sm-4">
                                                                    <span class="tileReview" style="background-color:{{projectReviewData[$index].color}}">
                                                                        <h3 class="tileReview">{{projectReviewData[$index].name}}</h3>
                                                                        <p>Metrics Submission Date:{{projectReviewData[$index].reviewDate}}</p>
                                                                    </span>
                                                                </div>
                                                                <div class="col-sm-4" ng-if="projectReviewData[$index+1]">
                                                                    <span class="tileReview" style="background-color:{{projectReviewData[$index+1].color}}">
                                                                        <h3 class="tileReview">{{projectReviewData[$index+1].name}}</h3>
                                                                        <p>Review Date:{{projectReviewData[$index+1].reviewDate}}</p>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane fade" id="tab_content4" aria-labelledby="Project-tab">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="x_panel">
                                                <div class="x_content">
                                                    <div class="col-md-12">
                                                        <div id="mGrid" ui-grid="savedReportsGrid" ui-grid-pagination="" class="gridDiv grid" ui-grid-cellNav ui-grid-resize-columns ui-grid-move-columns ui-grid-pinning ui-grid-selection ui-grid-exporter ui-grid-auto-resize style="height: 200px !important; width:100%">
                                                            <div class="grid-msg-overlay" ng-show="loading">
                                                                <div class="msg">
                                                                    <span>
                                                                        Loading Data...
                                                                        <i class="fa fa-spinner fa-spin"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                            <div class="grid-msg-overlay" ng-hide="loading || savedReportsGrid.data.length">
                                                                <div class="msg">
                                                                    <span>No Data</span>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane fade" id="tab_content5" aria-labelledby="Project-tab">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="x_panel">
                                                <div class="x_content">
                                                    <div class="col-md-12">
                                                        <div id="mGrid" ui-grid="uploadReportGrid" ui-grid-pagination="" class="gridDiv grid" ui-grid-cellNav ui-grid-resize-columns ui-grid-move-columns ui-grid-pinning ui-grid-selection  ui-grid-auto-resize style="height: 500px !important; width:100%">
                                                            <div class="grid-msg-overlay" ng-show="loading">
                                                                <div class="msg">
                                                                    <span>
                                                                        Loading Data...
                                                                        <i class="fa fa-spinner fa-spin"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                            <div class="grid-msg-overlay" ng-hide="loading || uploadReportGrid.data.length">
                                                                <div class="msg">
                                                                    <span>No Data</span>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>







