<div class="x_panel" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Users</h3>
            </div>
        </div>
    </div>
</div>
<div class="clearfix"></div>
<div class="ibox-content" ng-if="showSucessMessage">
    <div class="row">
        <div class="alert alert-success fade in" role="alert">
            {{responseMessage}}
        </div>
    </div>
</div>
<div class="ibox-content" ng-if="showErrorMessage">
    <div class="row">
        <div class="alert alert-error fade in" role="alert">
            {{responseMessage}}
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12 col-sm-6 col-xs-12">
        <div class="x_panel">
            <div class="x_content fix">
                <div class="form-group">
                    <div class="col-md-3"></div>
                    <label class="control-label col-md-3 col-sm-3 col-xs-2">Project Name</label>
                    <div class="col-md-3">
                        <select class="form-control" ng-model="selectedProjectReleaseDropdown" ng-change="getProjectUsers(selectedProjectReleaseDropdown)">
                            <option value="">Select a Project</option>
                            <option ng-repeat="project in projectsDropdown" value="{{project.Value}}">{{project.Text}}</option>
                        </select>
                    </div>
                    <div class="col-md-3"></div>
                </div>
                <br />
                <br />
                <div>
                    <div class="col-md-12">
                        <div id="grid1" ui-grid="gridOptions1" class="grid" style="height: auto !important;"></div>
                    </div>
                   
                    <button id="saveBtn" ng-click="saveData()" class="btn btn-success" style="margin-top: 2%">Save</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
